<% val rtTraceLevel: Int %>
<% val streamAllFiles: Boolean %>
<% val bashDollar: String = "$" %>
main() {
    java -jar ${bashDollar}{DX_FS_ROOT}/dxWDL.jar internal wfFragment ${bashDollar}{HOME} -traceLevel ${rtTraceLevel} ${if streamAllFiles "-streamAllFiles" else ""}
}

collect() {
    java -jar ${bashDollar}{DX_FS_ROOT}/dxWDL.jar internal collect ${bashDollar}{HOME} -traceLevel ${rtTraceLevel} ${if streamAllFiles "-streamAllFiles" else ""}
}