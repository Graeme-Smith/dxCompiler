<% val rtTraceLevel: Int %>
<% val streamAllFiles: Boolean %>
<% val bashDollar: String = "$" %>
main() {
    java -jar ${bashDollar}{DX_FS_ROOT}/dxWDL.jar internal wfFragment -traceLevel ${rtTraceLevel} ${if streamAllFiles "-streamAllFiles" else ""} ${bashDollar}{HOME}
}

collect() {
    java -jar ${bashDollar}{DX_FS_ROOT}/dxWDL.jar internal collect -traceLevel ${rtTraceLevel} ${if streamAllFiles "-streamAllFiles" else ""} ${bashDollar}{HOME}
}